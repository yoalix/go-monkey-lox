let fib = fn(n) {
  if (n < 2) {
    return n;
  }
  return fib(n - 1) + fib(n - 2);
};

let start = clock();
let result = fib(4);
let end = clock();
print("fib(4) = ", result);
print("time: ", end - start, "ms");



let map = fn(arr, f) {
    let iter = fn(arr, accumulated, f) {
        if (len(arr) == 0) {
            return accumulated;
        } else {
            return iter(rest(arr), push(accumulated, f(first(arr))), f);
        }
    };
    return iter(arr, [], f);
};

let numbers = [1, 1 + 1, 4 - 1, 2 * 2, 2 + 3, 12 / 2];
let fibmap = map(numbers, fib);
print(fibmap)
